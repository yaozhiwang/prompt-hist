{
  "manifest_version": 3,
  "name": "ChatGPT Prompt History",
  "description": "Prompt History with Arrow Keys for ChatGPT.",
  "version": "0.1.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
